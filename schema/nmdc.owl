    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Has an assembly been performed on a genome bin extracted from a metagenomic assembly?" .

meta:ref_biomaterial a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "ref_biomaterial" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Primary publication if isolated before genome publication; otherwise, primary genome report\"" .

meta:ref_db a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "ref_db" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"List of database(s) used for ORF annotation, along with version number and reference to website or publication\"" .

meta:rel_to_oxygen a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "rel_to_oxygen" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Is this organism an aerobe, anaerobe? Please note that aerobic and anaerobic are valid descriptors for microbial environments\"" .

meta:samp_collect_device a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "samp_collect_device" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "The method or device employed for collecting the sample" .

meta:samp_mat_process a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "samp_mat_process" ;
    rdfs:range meta:ControlledTermValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Any processing applied to the sample during or after retrieving the sample from environment. This field accepts OBI, for a browser of OBI (v 2018-02-12) terms please see http://purl.bioontology.org/ontology/OBI\"" .

meta:samp_size a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "samp_size" ;
    rdfs:range meta:QuantityValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Amount or size of sample (volume, mass or area) that was collected\"" .

meta:seq_meth a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "seq_meth" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Sequencing method used; e.g. Sanger, pyrosequencing, ABI-solid\"" .

meta:seq_quality_check a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "seq_quality_check" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Indicate if the sequence has been called by automatic systems (none) or undergone a manual editing procedure (e.g. by inspecting the raw data or chromatograms). Applied only for sequences that are not submitted to SRA,ENA or DRA\"" .

meta:sim_search_meth a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "sim_search_meth" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Tool used to compare ORFs with database, along with version and cutoffs used\"" .

meta:single_cell_lysis_appr a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "single_cell_lysis_appr" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Method used to free DNA from interior of the cell(s) or particle(s)" .

meta:single_cell_lysis_prot a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "single_cell_lysis_prot" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Name of the kit or standard protocol used for cell(s) or particle(s) lysis" .

meta:size_frac a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "size_frac" ;
    rdfs:range meta:QuantityValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Filtering pore size used in sample preparation" .

meta:sop a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "sop" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Standard operating procedures used in assembly and/or annotation of genomes, metagenomes or environmental sequences\"" .

meta:sort_tech a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "sort_tech" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Method used to sort/isolate cells or particles of interest" .

meta:source_mat_id a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "source_mat_id" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"A unique identifier assigned to a material sample (as defined by http://rs.tdwg.org/dwc/terms/materialSampleID, and as opposed to a particular digital record of a material sample) used for extracting nucleic acids, and subsequent sequencing. The identifier can refer either to the original material collected or to any derived sub-samples. The INSDC qualifiers /specimen_voucher, /bio_material, or /culture_collection may or may not share the same value as the source_mat_id field. For instance, the /specimen_voucher qualifier and source_mat_id may both contain 'UAM:Herps:14' , referring to both the specimen voucher and sampled tissue with the same identifier. However, the /culture_collection qualifier may refer to a value from an initial culture (e.g. ATCC:11775) while source_mat_id would refer to an identifier from some derived culture from which the nucleic acids were extracted (e.g. xatc123 or ark:/2154/R2).\"" .

meta:source_uvig a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "source_uvig" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Type of dataset from which the UViG was obtained" .

meta:specific_host a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "specific_host" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"If there is a host involved, please provide its taxid (or environmental if not actually isolated from the dead or alive host - i.e. a pathogen could be isolated from a swipe of a bench etc) and report whether it is a laboratory or natural host)\"" .

meta:study_alternate_identifiers a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "study_alternate identifiers" ;
    rdfs:domain meta:Study ;
    rdfs:range meta:string ;
    rdfs:subPropertyOf meta:alternate_identifiers .

meta:study_id a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "study_id" ;
    rdfs:domain meta:Study ;
    rdfs:range meta:string ;
    rdfs:subPropertyOf meta:id .

meta:study_name a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "study_name" ;
    rdfs:domain meta:Study ;
    rdfs:range meta:string ;
    rdfs:subPropertyOf meta:name .

meta:subspecf_gen_lin a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "subspecf_gen_lin" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"This should provide further information about the genetic distinctness of the sequenced organism by recording additional information e.g. serovar, serotype, biotype, ecotype, or any relevant genetic typing schemes like Group I plasmid. It can also contain alternative taxonomic information. It should contain both the lineage name, and the lineage rank, i.e. biovar:abc123\"" .

meta:target_gene a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "target_gene" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Targeted gene or locus name for marker gene studies" .

meta:target_subfragment a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "target_subfragment" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Name of subfragment of a gene or locus. Important to e.g. identify special regions on marker genes like V6 on 16S rRNA" .

meta:tax_class a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "tax_class" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Method used for taxonomic classification, along with reference database used, classification rank, and thresholds used to classify new genomes\"" .

meta:tax_ident a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "tax_ident" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "The phylogenetic marker(s) used to assign an organism name to the SAG or MAG" .

meta:time a owl:Class,
        meta:TypeDefinition ;
    rdfs:label "time" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:dateTime ;
            owl:onProperty meta:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "A time object represents a (local) time of day, independent of any particular day" ;
    skos:editorialNote "URI is dateTime because OWL reasoners don't work with straight date or time" .

meta:trna_ext_software a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "trna_ext_software" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Tools used for tRNA identification" .

meta:trnas a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "trnas" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "The total number of tRNAs identified from the SAG or MAG" .

meta:trophic_level a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "trophic_level" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Trophic levels are the feeding position in a food chain. Microbes can be a range of producers (e.g. chemolithotroph)" .

meta:uri a owl:Class,
        meta:TypeDefinition ;
    rdfs:label "uri" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty meta:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "a complete URI" .

meta:uriorcurie a owl:Class,
        meta:TypeDefinition ;
    rdfs:label "uriorcurie" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:anyURI ;
            owl:onProperty meta:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "a URI or a CURIE" .

meta:url a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "url" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "" .

meta:vir_ident_software a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "vir_ident_software" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Tool(s) used for the identification of UViG as a viral genome, software or protocol name  including version number, parameters, and cutoffs used\"" .

meta:virus_enrich_appr a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "virus_enrich_appr" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"List of approaches used to enrich the sample for viruses, if any\"" .

meta:votu_class_appr a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "votu_class_appr" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Cutoffs and approach used when clustering new UViGs in Rspecies-levelS vOTUs. Note that results from standard 95% ANI / 85% AF clustering should be provided alongside vOTUS defined from another set of thresholds, even if the latter are the ones primarily used during the analysis\"" .

meta:votu_db a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "votu_db" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Reference database (i.e. sequences not generated as part of the current study) used to cluster new genomes in \"\"species-level\"\" vOTUs, if any\"" .

meta:votu_seq_comp_appr a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "votu_seq_comp_appr" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Tool and thresholds used to compare sequences when computing \"\"species-level\"\" vOTUs\"" .

meta:wga_amp_appr a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "wga_amp_appr" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Method used to amplify genomic DNA in preparation for sequencing" .

meta:wga_amp_kit a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "wga_amp_kit" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Kit used to amplify genomic DNA in preparation for sequencing" .

meta:OntologyClass a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "ontology class" ;
    rdfs:subClassOf meta:NamedThing .

meta:Person a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "person" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:id ],
        meta:NamedThing ;
    skos:definition "represents a person, such as a researcher" .

meta:alt a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "alt" ;
    rdfs:range meta:QuantityValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Altitude is a term used to identify heights of objects such as airplanes, space shuttles, rockets, atmospheric balloons and heights of places such as atmospheric layers and clouds. It is used to measure the height of an object which is above the earthbs surface. In this context, the altitude measurement is the vertical distance between the earth's surface above sea level and the sampled position in the air\"" .

meta:biosample_set a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "biosample set" ;
    rdfs:domain meta:Database ;
    rdfs:range meta:Biosample .

meta:collection_date a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "collection_date" ;
    rdfs:range meta:TimestampValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"The time of sampling, either as an instance (single point in time) or interval. In case no exact time is available, the date/time can be right truncated i.e. all of these are valid times: 2008-01-23T19:23:10+00:00; 2008-01-23T19:23:10; 2008-01-23; 2008-01; 2008; Except: 2008-01; 2008 all are ISO8601 compliant\"" .

meta:data_object_set a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "data object set" ;
    rdfs:domain meta:Database ;
    rdfs:range meta:DataObject .

meta:description a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "description" ;
    rdfs:range meta:string ;
    skos:definition "a human-readable description of a thing" .

meta:ecosystem a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "ecosystem" ;
    rdfs:range meta:string ;
    rdfs:subPropertyOf meta:gold_path_field .

meta:ecosystem_category a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "ecosystem_category" ;
    rdfs:range meta:string ;
    rdfs:subPropertyOf meta:gold_path_field .

meta:ecosystem_subtype a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "ecosystem_subtype" ;
    rdfs:range meta:string ;
    rdfs:subPropertyOf meta:gold_path_field .

meta:ecosystem_type a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "ecosystem_type" ;
    rdfs:range meta:string ;
    rdfs:subPropertyOf meta:gold_path_field .

meta:elev a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "elev" ;
    rdfs:range meta:QuantityValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Elevation of the sampling site is its height above a fixed reference point, most commonly the mean sea level. Elevation is mainly used when referring to points on the earth's surface, while altitude is used for points above the surface, such as an aircraft in flight or a spacecraft in orbit\"" .

meta:experimental_factor a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "experimental_factor" ;
    rdfs:range meta:ControlledTermValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Experimental factors are essentially the variable aspects of an experiment design which can be used to describe an experiment, or set of experiments, in an increasingly detailed manner. This field accepts ontology terms from Experimental Factor Ontology (EFO) and/or Ontology for Biomedical Investigations (OBI). For a browser of EFO (v 2.95) terms, please see http://purl.bioontology.org/ontology/EFO; for a browser of OBI (v 2018-02-12) terms please see http://purl.bioontology.org/ontology/OBI\"" .

meta:file_size a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "file_size" ;
    rdfs:range meta:string ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "units should be bytes.. may be overkill to allow different units" .

meta:geo_loc_name a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "geo_loc_name" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"The geographical origin of the sample as defined by the country or sea name followed by specific region name. Country or sea names should be chosen from the INSDC country list (http://insdc.org/country.html), or the GAZ ontology (v 1.512) (http://purl.bioontology.org/ontology/GAZ)\"" .

meta:has_boolean_value a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "has boolean value" ;
    rdfs:range meta:boolean ;
    skos:definition "Links a quantity value to a boolean" .

meta:investigation_type a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "investigation_type" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Nucleic Acid Sequence Report is the root element of all MIGS/MIMS compliant reports as standardized by Genomic Standards Consortium. This field is either eukaryote,bacteria,virus,plasmid,organelle, metagenome,mimarks-survey, mimarks-specimen, metatranscriptome, single amplified genome, metagenome-assembled genome, or uncultivated viral genome\"" .

meta:language a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "language" ;
    rdfs:range meta:string ;
    skos:definition "Should use standard codes, e.g. \"en\"" .

meta:latitude a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "latitude" ;
    rdfs:domain meta:GeolocationValue ;
    rdfs:range meta:float ;
    skos:definition "latitude" .

meta:longitude a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "longitude" ;
    rdfs:domain meta:GeolocationValue ;
    rdfs:range meta:float ;
    skos:definition "longitude" .

meta:project_name a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "project_name" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Name of the project within which the sequencing was organized" .

meta:specific_ecosystem a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "specific_ecosystem" ;
    rdfs:range meta:string ;
    rdfs:subPropertyOf meta:gold_path_field .

meta:submitted_to_insdc a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "submitted_to_insdc" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"Depending on the study (large-scale e.g. done with next generation sequencing technology, or small-scale) sequences have to be submitted to SRA (Sequence Read Archive), DRA (DDBJ Read Archive) or via the classical Webin/Sequin systems to Genbank, ENA and DDBJ. Although this field is mandatory, it is meant as a self-test field, therefore it is not necessary to include this field in contextual data submitted to databases\"" .

meta:BiosampleProcessing a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "biosample processing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom meta:Biosample ;
            owl:onProperty meta:has_input ],
        meta:NamedThing ;
    skos:definition "A process that takes one or more biosamples as inputs and generates one or as outputs. Examples of outputs include samples cultivated from another sample or data objects created by instruments runs." .

meta:Database a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "database" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:Biosample ;
            owl:onProperty meta:biosample_set ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:DataObject ;
            owl:onProperty meta:data_object_set ] ;
    skos:definition "top level holder class" .

meta:TimestampValue a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "timestamp value" ;
    rdfs:subClassOf meta:AttributeValue ;
    skos:definition "A value that is a timestamp. The range should be ISO-8601" .

meta:boolean a owl:Class,
        meta:TypeDefinition ;
    rdfs:label "boolean" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:boolean ;
            owl:onProperty meta:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "A binary (true or false) value" .

meta:depth a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "depth" ;
    rdfs:range meta:TextValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "Please refer to the definitions of depth in the environmental packages" .

meta:double a owl:Class,
        meta:TypeDefinition ;
    rdfs:label "double" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:double ;
            owl:onProperty meta:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "A real number that conforms to the xsd:double specification" .

meta:env_broad_scale a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "env_broad_scale" ;
    rdfs:range meta:ControlledTermValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"In this field, report which major environmental system your sample or specimen came from. The systems identified should have a coarse spatial grain, to provide the general environmental context of where the sampling was done (e.g. were you in the desert or a rainforest?). We recommend using subclasses of ENVOUs biome class: http://purl.obolibrary.org/obo/ENVO_00000428. Format (one term): termLabel [termID], Format (multiple terms): termLabel [termID]|termLabel [termID]|termLabel [termID]. Example: Annotating a water sample from the photic zone in middle of the Atlantic Ocean, consider: oceanic epipelagic zone biome [ENVO:01000033]. Example: Annotating a sample from the Amazon rainforest consider: tropical moist broadleaf forest biome [ENVO:01000228]. If needed, request new terms on the ENVO tracker, identified here: http://www.obofoundry.org/ontology/envo.html\"" .

meta:env_local_scale a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "env_local_scale" ;
    rdfs:range meta:ControlledTermValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"In this field, report the entity or entities which are in your sample or specimenUs local vicinity and which you believe have significant causal influences on your sample or specimen. Please use terms that are present in ENVO and which are of smaller spatial grain than your entry for env_broad_scale. Format (one term): termLabel [termID]; Format (multiple terms): termLabel [termID]|termLabel [termID]|termLabel [termID]. Example: Annotating a pooled sample taken from various vegetation layers in a forest consider: canopy [ENVO:00000047]|herb and fern layer [ENVO:01000337]|litter layer [ENVO:01000338]|understory [01000335]|shrub layer [ENVO:01000336]. If needed, request new terms on the ENVO tracker, identified here: http://www.obofoundry.org/ontology/envo.html\"" .

meta:env_medium a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "env_medium" ;
    rdfs:range meta:ControlledTermValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "\"In this field, report which environmental material or materials (pipe separated) immediately surrounded your sample or specimen prior to sampling, using one or more subclasses of ENVOUs environmental material class: http://purl.obolibrary.org/obo/ENVO_00010483. Format (one term): termLabel [termID]; Format (multiple terms): termLabel [termID]|termLabel [termID]|termLabel [termID]. Example: Annotating a fish swimming in the upper 100 m of the Atlantic Ocean, consider: ocean water [ENVO:00002151]. Example: Annotating a duck on a pond consider: pond water [ENVO:00002228]|air ENVO_00002005. If needed, request new terms on the ENVO tracker, identified here: http://www.obofoundry.org/ontology/envo.html\"" .

meta:has_output a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "has output" ;
    rdfs:range meta:string ;
    skos:definition "An output biosample to a processing step" .

meta:has_unit a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "has unit" ;
    rdfs:range meta:Unit ;
    skos:definition "Links a quantity value to a unit" .

meta:lat_lon a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "lat_lon" ;
    rdfs:range meta:GeolocationValue ;
    rdfs:subPropertyOf meta:attribute ;
    skos:definition "The geographical origin of the sample as defined by latitude and longitude. The values should be reported in decimal degrees and in WGS84 system" .

meta:Unit a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "unit" .

meta:has_input a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "has input" ;
    rdfs:domain meta:NamedThing ;
    rdfs:range meta:string ;
    skos:definition "An input to a process." .

meta:has_numeric_value a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "has numeric value" ;
    rdfs:range meta:string ;
    skos:definition "Links a quantity value to a number" .

meta:part_of a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "part of" ;
    rdfs:domain meta:NamedThing ;
    rdfs:range meta:string ;
    skos:definition "Links a resource to another resource that either logically or physically includes it." .

meta:DataObject a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "data object" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:file_size ],
        meta:NamedThing ;
    skos:definition "An object that primarily consists of symbols that represent information.   Files, records, and omics data are examples of data objects. " .

meta:float a owl:Class,
        meta:TypeDefinition ;
    rdfs:label "float" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:float ;
            owl:onProperty meta:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "A real number that conforms to the xsd:float specification" .

meta:OmicsProcessing a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "omics processing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom meta:string ;
            owl:onProperty meta:alternate_identifiers ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:id ],
        [ a owl:Restriction ;
            owl:allValuesFrom meta:DataObject ;
            owl:onProperty meta:has_output ],
        [ a owl:Restriction ;
            owl:allValuesFrom meta:Study ;
            owl:onProperty meta:part_of ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:name ],
        meta:BiosampleProcessing ;
    skos:definition "The methods and processes used to generate omics data from a biosample or organism." .

meta:Study a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "study" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom meta:string ;
            owl:onProperty meta:alternate_identifiers ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:id ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:TextValue ;
            owl:onProperty meta:submitted_to_insdc ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:name ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:TextValue ;
            owl:onProperty meta:project_name ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:ControlledTermValue ;
            owl:onProperty meta:experimental_factor ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:TextValue ;
            owl:onProperty meta:investigation_type ],
        meta:NamedThing ;
    skos:definition "A study summarizes the overall goal of a research initiative and outlines the key objective of its underlying projects.  " .

meta:gold_path_field a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "gold_path_field" ;
    rdfs:range meta:string ;
    rdfs:subPropertyOf meta:attribute .

meta:has_raw_value a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "has raw value" ;
    rdfs:domain meta:AttributeValue ;
    rdfs:range meta:string ;
    skos:definition "The value that was specified for an annotation in raw form, i.e. a string. E.g. \"2 cm\" or \"2-4 cm\"" .

meta:GeolocationValue a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "geolocation value" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:float ;
            owl:onProperty meta:longitude ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:has_raw_value ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:float ;
            owl:onProperty meta:latitude ],
        meta:AttributeValue ;
    skos:definition "A normalized value for a location on the earth's surface" .

meta:alternate_identifiers a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "alternate identifiers" ;
    rdfs:range meta:string ;
    skos:definition "Non-primary identifiers" .

meta:name a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "name" ;
    rdfs:range meta:string ;
    skos:definition "A human readable label for an entity" .

meta:NamedThing a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "named thing" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom meta:string ;
            owl:onProperty meta:part_of ],
        [ a owl:Restriction ;
            owl:allValuesFrom meta:string ;
            owl:onProperty meta:alternate_identifiers ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:id ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom meta:string ;
            owl:onProperty meta:has_input ] ;
    skos:definition "a databased entity or concept/class" .

meta:AttributeValue a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "attribute value" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:has_raw_value ] ;
    skos:definition "The value for any value of a attribute for a sample. This object can hold both the un-normalized atomic value and the structured value" .

meta:QuantityValue a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "quantity value" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:double ;
            owl:onProperty meta:has_numeric_value ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:has_raw_value ],
        [ a owl:Restriction ;
            owl:allValuesFrom meta:Unit ;
            owl:onProperty meta:has_unit ],
        meta:AttributeValue ;
    skos:definition "A simple quantity, e.g. 2cm" .

meta:id a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "id" ;
    rdfs:range meta:string ;
    skos:definition "A unique identifier for a thing. Must be either a CURIE shorthand for a URI or a complete URI" .

meta:Biosample a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "biosample" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass meta:ControlledTermValue ;
            owl:onProperty meta:env_medium ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:QuantityValue ;
            owl:onProperty meta:elev ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:ecosystem_category ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:ecosystem_type ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:name ],
        [ a owl:Restriction ;
            owl:allValuesFrom meta:string ;
            owl:onProperty meta:alternate_identifiers ],
        [ a owl:Restriction ;
            owl:onClass meta:ControlledTermValue ;
            owl:onProperty meta:env_broad_scale ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:onClass meta:ControlledTermValue ;
            owl:onProperty meta:env_local_scale ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:TextValue ;
            owl:onProperty meta:depth ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:TimestampValue ;
            owl:onProperty meta:collection_date ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:id ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:ecosystem_subtype ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:ecosystem ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:TextValue ;
            owl:onProperty meta:geo_loc_name ],
        [ a owl:Restriction ;
            owl:onClass meta:GeolocationValue ;
            owl:onProperty meta:lat_lon ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:specific_ecosystem ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:QuantityValue ;
            owl:onProperty meta:alt ],
        meta:NamedThing ;
    skos:definition "A material sample. It may be environmental (encompassing many organisms) or isolate or tissue.   An environmental sample containing genetic material from multiple individuals is commonly referred to as a biosample.  " .

meta:ControlledTermValue a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "controlled term value" ;
    rdfs:subClassOf meta:AttributeValue ;
    skos:definition "A controlled term or class from an ontology" .

meta:TypeDefinition a owl:Class ;
    rdfs:label "type_definition" ;
    skos:definition "A data type definition." .

meta:topValue a owl:DatatypeProperty ;
    rdfs:label "value" .

meta:ClassDefinition a owl:Class ;
    rdfs:label "class_definition" ;
    skos:definition "the definition of a class or interface" .

meta:string a owl:Class,
        meta:TypeDefinition ;
    rdfs:label "string" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange xsd:string ;
            owl:onProperty meta:topValue ;
            owl:qualifiedCardinality 1 ] ;
    skos:definition "A character string" .

meta:TextValue a owl:Class,
        meta:ClassDefinition ;
    rdfs:label "text value" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass meta:string ;
            owl:onProperty meta:language ],
        meta:AttributeValue ;
    skos:definition "A basic string value" .

meta:attribute a owl:ObjectProperty,
        meta:SlotDefinition ;
    rdfs:label "attribute" ;
    rdfs:range meta:string ;
    skos:definition "A attribute of a biosample. Examples: depth, habitat, material. For NMDC, attributes SHOULD be mapped to terms within a MIxS template" .

meta:SlotDefinition a owl:Class ;
    rdfs:label "slot_definition" ;
    skos:definition "the definition of a property or a slot" .



